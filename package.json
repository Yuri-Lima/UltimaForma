  "license": "MIT",
  "scripts": {
    "start:api": "nx serve api",
    "start:web": "nx serve web",
    "start:worker": "nx serve worker",
    "watch:api": "nx serve api",
    "watch:web": "nx serve web",
    "watch:worker": "nx serve worker",
    "watch": "nx run-many -t serve -p api,web,worker",
    "build:api": "nx build api",
    "build:web": "nx build web",
    "build:worker": "nx build worker",
    "db:migration:create": "typeorm migration:create",
    "db:migration:generate": "TS_NODE_PROJECT=apps/api/tsconfig.app.json node -r ts-node/register ./node_modules/typeorm/cli.js migration:generate -d ./apps/api/src/db/data-source.ts",
    "db:migration:run": "TS_NODE_PROJECT=apps/api/tsconfig.app.json node -r ts-node/register ./node_modules/typeorm/cli.js migration:run -d ./apps/api/src/db/data-source.ts",
    "db:migration:revert": "TS_NODE_PROJECT=apps/api/tsconfig.app.json node -r ts-node/register ./node_modules/typeorm/cli.js migration:revert -d ./apps/api/src/db/data-source.ts",
    "db:migration:show": "TS_NODE_PROJECT=apps/api/tsconfig.app.json node -r ts-node/register ./node_modules/typeorm/cli.js migration:show -d ./apps/api/src/db/data-source.ts",
    "db:ensure-embeddings": "TS_NODE_PROJECT=apps/api/tsconfig.app.json node -r ts-node/register apps/api/src/db/ensure-embeddings.ts",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down"
  },
  "private": true,
